<div class="breadcome-area">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="breadcome-list">
                    <div class="row">
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                            <div class="breadcomb-wp">
                                <div class="breadcomb-icon">
                                    <i class="icon nalika-home"></i>
                                </div>
                                <div class="breadcomb-ctn">
                                    <h2>Product Edit</h2>
                                    <p>Welcome To Cherish <span class="bread-ntd"></span></p>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                            <div class="breadcomb-report">
                                <button data-toggle="tooltip" data-placement="left" title="Download Report"
                                    class="btn"><i class="icon nalika-download"></i></button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Single pro tab start-->
<div class="single-product-tab-area mg-b-30">
    <!-- Single pro tab review Start-->
    <div class="single-pro-review-area">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="review-tab-pro-inner">
                        <ul id="myTab3" class="tab-review-design">
                            <li class="active"><a href="#description"><i class="icon nalika-edit"
                                        aria-hidden="true"></i>Product Edit</a></li>
                            </li>
                        </ul>



                        <div id="myTabContent" class="tab-content custom-product-edit">
                            <div class="product-tab-list tab-pane fade active in" id="description">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                        {{#products}}
                                        <form action="/products/productsEdit/{{ProductID}}/comfirm" method="POST"
                                            id="editProduct">
                                            
                                            <div class="review-content-section">

                                                <div class="input-group mg-b-pro-edt">
                                                    <span class="input-group-addon"></span>
                                                    <input type="text" class="form-control" placeholder="Product ID"
                                                        value="{{ProductID}}" name="ProductID">
                                                </div>

                                                <div class="input-group mg-b-pro-edt">
                                                    <span class="input-group-addon"></span>
                                                    <input type="text" class="form-control" placeholder="Product Name"
                                                        value="{{ProductName}}" name="ProductName">
                                                </div>
                                                <div class="input-group mg-b-pro-edt">
                                                    <span class="input-group-addon"></span>
                                                    <input type="text" class="form-control" placeholder="Category"
                                                        value="{{Category}}" name="Category">
                                                </div>
                                                <div class="input-group mg-b-pro-edt">
                                                    <span class="input-group-addon"></span>
                                                    <input type="text" class="form-control" placeholder="BrandID"
                                                        value="{{BrandID}}" name="BrandID">
                                                </div>
                                                <div class="input-group mg-b-pro-edt">
                                                    <span class="input-group-addon"></span>
                                                    <input type="text" class="form-control" placeholder="ProducingYear"
                                                        value="{{ProducingYear}}" name="ProducingYear">
                                                </div>
                                                <div class="input-group mg-b-pro-edt">
                                                    <span class="input-group-addon"></span>
                                                    <input type="text" class="form-control" placeholder="Color"
                                                        value="{{Color}}" name="Color">
                                                </div>
                                                <div class="input-group mg-b-pro-edt">
                                                    <span class="input-group-addon"></span>
                                                    <input type="text" class="form-control" placeholder="Weight"
                                                        value="{{Weight}}" name="Weight">
                                                </div>
                                                <div class="input-group mg-b-pro-edt">
                                                    <span class="input-group-addon"></span>
                                                    <input type="text" class="form-control" placeholder="Count"
                                                        value="{{Count}}" name="Count">
                                                </div>
                                                <div class="input-group mg-b-pro-edt">
                                                    <span class="input-group-addon"></span>
                                                    <input type="text" class="form-control" placeholder="Price"
                                                        value="{{Price}}" name="Price">
                                                </div>
                                                <div class="input-group mg-b-pro-edt">
                                                    <span class="input-group-addon"></span>
                                                    <input type="text" class="form-control" placeholder="SalePrice"
                                                        value="{{SalePrice}}" name="SalePrice">
                                                </div>
                                                <div class="input-group mg-b-pro-edt">
                                                    <span class="input-group-addon"></span>
                                                    <input type="text" class="form-control" placeholder="Description"
                                                        value="{{Description}}" name="Description">
                                                </div>
                                                <div class="input-group mg-b-pro-edt">
                                                    <span class="input-group-addon"></span>
                                                    <input type="hidden" class="form-control" placeholder="Image"
                                                        value="{{Image}}" name="Image">
                                                </div>
                                                <div class="text-center custom-pro-edt-ds">

                                                    <button type="submit"
                                                        class="btn btn-ctl-bt waves-effect waves-light m-r-10">Save
                                                    </button>
                                                    <a href="/products">
                                                        <button type="button"
                                                            class="btn btn-ctl-bt waves-effect waves-light">Discard
                                                        </button>
                                                    </a>
                                                </div>
                                            </div>
                                            
                                        </form>
                                        {{/products}}
                                        
                                    </div>
                                </div>
                                {{#if message }}
                                <h4 class="alert alert-danger mt-4">{{message}}</h4>
                                {{/if}}
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>